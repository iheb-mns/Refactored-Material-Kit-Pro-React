{
  "name": "dwm-frontend",
  "version": "1.0.0",
  "description": "dwm-frontend",
  "license": "ISC",
  "main": "pages/index.js",
  "scripts": {
    "build": "next build",
    "dev": "env-cmd ./.config/dev.env next",
    "prod": "npm run build && env-cmd ./.config/prod.env node server.js",
    "start": "node server.js",
    "jest": "jest --watch --runInBand",
    "jest-ci": "jest --watch --runInBand",
    "lint": "eslint . --ext=js,jsx",
    "lint-fix": "eslint . --ext=js,jsx --fix",
    "deploy": "now rm dinewithme -y && now && now alias && now scale dinewithme.app 1 auto"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@types/googlemaps": "^3.30.16",
    "@types/markerclustererplus": "^2.1.33",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "animate.css": "^3.7.0",
    "apollo-boost": "^0.1.22",
    "axios": "^0.21.1",
    "babel-plugin-css-in-js": "^1.5.1",
    "classnames": "^2.2.6",
    "graphql": "^14.0.2",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "next-compose-plugins": "^2.1.1",
    "next-optimized-images": "^2.1.0",
    "nouislider": "^12.1.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-apollo": "^2.3.2",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.6.1",
    "react-google-maps": "^9.4.5",
    "react-image-gallery": "^0.8.12",
    "react-jss": "^8.6.1",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.2",
    "react-swipeable-views": "^0.13.0",
    "react-tagsinput": "^3.19.0",
    "svg-sprite-loader": "^4.1.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "tachyons": "^4.11.1",
    "unstated": "^2.1.1",
    "webp-loader": "^0.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.8.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-graphql": "^3.0.1",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "now": "^12.1.12",
    "prettier": "^1.15.1",
    "pretty-quick": "^1.8.0"
  },
  "prettier": {
    "eslintIntegration": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "arrowParens": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jest/globals": true
    },
    "settings": {
      "react": {
        "version": "^16.7.0-alpha.2"
      }
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "jest",
      "graphql"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0
    }
  },
  "now": {
    "version": 1,
    "name": "dinewithme",
    "dotenv": "./.config/prod.env",
    "env": {
      "NODE_ENV": "production",
      "SERVICE_WORKER": "true"
    },
    "alias": "dinewithme.app"
  }
}
